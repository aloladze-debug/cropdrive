{
  "name": "cropdrive-op-advisor",
  "version": "1.0.0",
  "description": "AI-powered palm oil farm analysis platform for Malaysian farmers",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "export": "next build && next export",
    "analyze": "next build --analyze",
    "clean": "rm -rf .next out node_modules/.cache",
    "setup": "npm install && npm run type-check",
    "firebase:setup": "node firebase-setup.js",
    "stripe:setup": "node stripe-setup.js",
    "db:init": "node database-init.js",
    "setup:all": "npm run firebase:setup && npm run stripe:setup && npm run db:init"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@stripe/stripe-js": "^2.1.11",
    "@types/node": "^20.8.10",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "autoprefixer": "^10.4.16",
    "dotenv": "^16.3.1",
    "eslint": "^8.53.0",
    "eslint-config-next": "^14.0.2",
    "firebase": "^12.4.0",
    "firebase-admin": "^12.0.0",
    "framer-motion": "^10.18.0",
    "lucide-react": "^0.547.0",
    "next": "^14.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-firebase-hooks": "^5.1.1",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^4.11.0",
    "react-type-animation": "^3.2.0",
    "stripe": "^14.6.0",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5"
  }
}
